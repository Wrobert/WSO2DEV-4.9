<?xml version="1.0" encoding="UTF-8"?>
<messageProcessor
  class="org.apache.synapse.message.processor.impl.forwarder.ScheduledMessageForwardingProcessor"
  messageStore="Vbe2SapMessageStore" name="Vbe2SapMessageProcessor"
  targetEndpoint="gov:bls/sap/endpoint/Esb2SapEndpoint.xml" xmlns="http://ws.apache.org/ns/synapse">
  <parameter name="client.retry.interval">5000</parameter>
  <parameter name="max.delivery.attempts">4</parameter>
  <parameter name="member.count">1</parameter>
  <parameter name="message.processor.reply.sequence">MessageStoreReplySequence</parameter>
  <parameter name="message.processor.fault.sequence">MessageStoreFaultSequence</parameter>
  <parameter name="max.delivery.drop">Disabled</parameter>
  <parameter name="interval">5000</parameter>
  <parameter name="message.processor.deactivate.sequence">MessageStoreDeactivateSequence</parameter>
  <parameter name="is.active">true</parameter>
</messageProcessor>
